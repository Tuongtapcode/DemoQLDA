            steps {
                git branch: 'main', url: 'https://github.com/Tuongtapcode/DemoQLDA.git'
            }
        }

        stage('Hello') {
            steps {
                echo 'âœ… Jenkins pipeline is working!'
                sh 'ls -la'
            }
        }
    }
}
